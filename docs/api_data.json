[  {    "type": "post",    "url": "/activate",    "title": "Activate registered user",    "version": "1.0.0",    "name": "ActivateUser",    "group": "Users",    "permission": [      {        "name": "none"      }    ],    "description": "<p>Attempts to activate the user, and, if successful, returns authentication token and activated user info</p> ",    "parameter": {      "fields": {        "Query": [          {            "group": "Query",            "type": "<p>String</p> ",            "optional": false,            "field": "token",            "description": "<p>Token from the user's email</p> "          }        ]      }    },    "examples": [      {        "title": "Example usage:",        "content": "curl -i -X POST -H 'Accept-Version: *' -H 'Accept: application/vnd.api+json' -H 'Accept-Encoding: gzip, deflate' \\\nhttps://api-users.sandbox.matic.ninja/api/users/activate?token=xxx",        "type": "curl"      }    ],    "filename": "src/endpoints/activate.js",    "groupTitle": "Users",    "success": {      "fields": {        "Code 200": [          {            "group": "Code 200",            "type": "<p>Object</p> ",            "optional": false,            "field": "meta",            "description": "<p>response meta information</p> "          },          {            "group": "Code 200",            "type": "<p>String</p> ",            "optional": false,            "field": "meta.id",            "description": "<p>request id</p> "          },          {            "group": "Code 200",            "type": "<p>String</p> ",            "optional": false,            "field": "meta.jwt",            "description": "<p>jsonwebtoken to be used for further authentication</p> "          },          {            "group": "Code 200",            "type": "<p>Object</p> ",            "optional": false,            "field": "data",            "description": "<p>response data</p> "          },          {         