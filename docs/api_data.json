rror:",          "content": "HTTP/1.1 400 BadRequest\n{\n\t\"meta\": {\n\t\t\"id\": \"request-id\"\n\t},\n\t\"errors\": [{\n\t\t\"status\": \"ValidationError\",\n\t\t\"code\": 400,\n\t\t\"title\": \"short description of the errors\",\n\t\t\"detail\": [{\n\t\t\t\"text\": \"invalid input type\",\n\t\t\t\"code\": 400,\n\t\t\t\"field\": \"data.type\"\n\t\t}]\n\t}]\n}",          "type": "json"        }      ]    }  },  {    "type": "patch",    "url": "/:id/ban",    "title": "Bans or unbans specified user",    "version": "1.0.0",    "name": "BanUser",    "group": "Users",    "permission": [      {        "name": "admin"      }    ],    "description": "<p>Locks or unlocks account of the specified user based on the body payload</p> ",    "parameter": {      "fields": {        "Params": [          {            "group": "Params",            "type": "<p>String</p> ",            "optional": false,            "field": "id",            "description": "<p>username we are trying to lock/unlock</p> "          }        ],        "Body": [          {            "group": "Body",            "type": "<p>Object</p> ",            "optional": false,            "field": "data",            "description": "<p>container</p> "          },          {            "group": "Body",            "type": "<p>String</p> ",            "allowedValues": [              "\"user\""            ],            "optional": false,            "field": "data.type",            "description": "<p>we are modifying this object type, must be 'user'</p> "          },          {            "group": "Body",            "type": "<p>Object</p> ",            "optional": false,            "field": "data.attributes",            "description": "<p>container for attributes</p> "          },          {            "group": "Body",            "type": "<p>Boolean</p> ",            "optional": false,            "field": "data.attributes.ban",            "description": "<p>when <c